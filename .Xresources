!-------------------------------------------------------------------------------
! URxvt settings
! Colours lifted from Solarized (http://ethanschoonover.com/solarized)
! More info at:
! http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod
!-------------------------------------------------------------------------------
URxvt.termName: rxvt-unicode-256color
!URxvt.termName: xterm-256color

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.shading: 0
URxvt.fading:               0
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.saveLines:            20000
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Fonts
URxvt.allow_bold:           false
/* URxvt.font:                 -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1 */

! Fix font space
URxvt*letterSpace: -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common:      default,matcher,resize-font,url-select,keyboard-select,clipboard
URxvt.matcher.button:       1
URxvt.urlLauncher:          google-chrome-stable
!! url-select
URxvt.keysym.M-u:   perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape:  perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:      M-9
URxvt.resize-font.bigger:       M-0
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
!! tabbed
!URxvt.tabbed.new-button: true
!URXvt.tabbed.reopen-on-close: yes
!URxvt.keysym.Control-Shift-t: perl:tabbedex:new_tab
!URxvt.keysym.Shifft-Right: perl:tabbedex:next_tab
!URxvt.keysym.Shift-Left: perl:tabbedex:prev_tab
!URxvt.tabbed.no-tabbedex-keys: true

!URxvt.tabbed.tabbar-fg: 2
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg: 3
!URxvt.tabbed.tab-bg: 0

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #4e5ab7
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

